# EMBA - SBOM Native Profile (NO Docker, NO QEMU)
# Обязательно отключаем Docker и эмуляцию

export USE_DOCKER=0
export IN_DOCKER=0
export QEMULATION=0

export FORMAT_LOG=1
export THREADED=1
export SHORT_PATH=1
export HTML=1

export SBOM_MINIMAL=0
export SBOM_UNTRACKED_FILES=1
export DISABLE_DEEP=1
export DEEP_EXT_DEPTH=1
