# ТОЛЬКО необходимые модули для SBOM (БЕЗ S115/S116!)
export SELECT_MODULES+=( "S06" "S08" "S09" "S24" "S25" "F14" "F15" )

export VEX_METRICS=0
export DISABLE_STATUS_BAR=0
export SILENT=1
export DISABLE_NOTIFICATIONS=1
export DISABLE_DOTS=1
